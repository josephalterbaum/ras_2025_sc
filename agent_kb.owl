<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://ais.tum.de/myyoghurt_kb.owl"
         xmlns="http://ais.tum.de/myyoghurt_kb.owl#">

<owl:Ontology rdf:about="http://ais.tum.de/myyoghurt_kb.owl"/>

<owl:ObjectProperty rdf:about="#has_component">
  <rdfs:domain rdf:resource="#SystemComponent"/>
  <rdfs:range rdf:resource="#System"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#System">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#SystemComponent">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>


</rdf:RDF>
